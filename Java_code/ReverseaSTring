class ReverseAStringWithoutusingInbuitFunctions{
    public static void main(String[] args){
        /*
        Declared a string with some name
        interface 
        */
        String str = "Niranjan";
        StringBuilder str2 = new StringBuilder();
        str2.append(str);
        str2 = str2.reverse();
        System.out.println(str2);
    }
}
